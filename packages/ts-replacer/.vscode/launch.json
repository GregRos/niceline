{
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Run check-for-duplicates",
      "runtimeExecutable": "tsx",
      "runtimeArgs": [
        "--tsconfig",
        "tools/tsconfig.json",
        "./tools/check-for-duplicates"
      ],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "RUN umm",
      "runtimeExecutable": "tsx",
      "program": "${workspaceFolder}/tools/umm.ts",
      "runtimeArgs": ["--tsconfig", "${workspaceFolder}/tools/tsconfig.json"],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "env": {
        "NODE_ENV": "test"
      }
    },
    {
      "type": "node",
      "request": "launch",
      "name": "TEST chars",
      "program": "${workspaceFolder}/../../node_modules/jest/bin/jest.js",
      "args": [
        "--runInBand",
        "--config",
        "jest.config.mjs",
        "--testPathPattern",
        "schema.test.ts"
      ],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "env": {
        "NODE_ENV": "test"
      }
    }
  ]
}
