{
  "$id": "niceline://namespace",
  "$defs": {
    "Namespace": {
      "type": "object",
      "patternProperties": {
        "^.$": {
          "anyOf": [
            {
              "type": "array",
              "additionalItems": false,
              "items": [
                {
                  "type": "string"
                }
              ],
              "minItems": 1,
              "prefixItems": [
                {
                  "anyOf": [
                    {
                      "type": "string",
                      "title": "a {string}.",
                      "pattern": "^\\{.+\\}$"
                    },
                    {
                      "type": "null"
                    }
                  ],
                  "title": "AsciiId or null"
                }
              ],
              "title": "Array of aliases"
            },
            {
              "$ref": "#/$defs/Namespace"
            }
          ]
        }
      },
      "title": "Namespace",
      "description": "Recursive, namespaced dictionary of Unicode to ASCII codes.",
      "additionalProperties": {
        "$ref": "#/$defs/Namespace"
      }
    }
  },
  "$ref": "#/$defs/Namespace"
}