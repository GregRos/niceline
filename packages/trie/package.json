{
  "name": "@niceline/trie",
  "version": "0.1.0",
  "description": "A monomorphic trie with string[] keys and string values",
  "repository": {
    "type": "git",
    "url": "github:GregRos/niceline",
    "directory": "packages/trie"
  },
  "license": "MIT",
  "author": "GregRos",
  "contributors": [
    {
      "name": "GregRos",
      "email": "gregros@gregros.dev"
    }
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.js",
      "default": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "prepack": "shx rm -rf **/tsconfig*",
    "build": "tsc -b .",
    "build:clean": "run-s clean build",
    "watch": "tsc -b . --watch",
    "clean": "shx rm -rf dist dist_test",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "tidy": "yarn install && run-s lint:fix build:clean test"
  },
  "dependencies": {
    "doddle": "^7.1.6",
    "what-are-you": "^0.1.13"
  },
  "devDependencies": {
    "@swc/core": "^1.5.25",
    "@swc/jest": "^0.2.36",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.4",
    "@types/node": "^24.9.1",
    "jest": "^29.7.0",
    "lodash": "^4.17.21",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "packageManager": "yarn@4.2.2"
}
